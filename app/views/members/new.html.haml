= link_to "Back", members_path
%h1 New Member
= form_for :member, url: members_path do |f|
  = f.label :firstname
  = f.text_field :firstname
  %br
  = f.label :lastname
  = f.text_field :lastname
  %br
  = f.label :email
  = f.text_field :email
  %br
  = f.label :grade
  = f.text_field :grade
  %br
  = f.submit